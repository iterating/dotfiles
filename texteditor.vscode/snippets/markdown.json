{
	// Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

// Make a keyboard symbol in the selected
	"kbd": {
		"prefix" : ["kbd"],
		"body": ["<kbd>$1</kbd> $0"],
		"description": "type a Keyboard button"
	},


// Fold a selection and name it
	"fold.selected": {
		"body":
			["<details><summary>$1",
			"  </summary>",
			"  ${TM_SELECTED_TEXT}",
			"  </details>",
			"$0"],
		"description": "Creates a toggle fold on the selection"
	},

	// Fold a new section
	"fold.new": {
		"body":
			["<details><summary>$1",
			"  </summary>",
			"  $2",
			"  </details>",
			"  $0"],
		"description": "Creates a new toggle fold"
	},

// Make a page break
	"pagebreak": {
		"prefix" : ["Page Break"],
		"body": " <div style="page-break-after: always"></div> ",
		"description": "Creates a Page Break"
	},

	// Insert a iframe in selection

	"iframe": {
			"prefix" : ["IFrame"],
			"body": ["<iframe src=$1></iframe>$0"],
			"description": "Inserts an iframe"
	}


	// Fold a new section
	"sub": {
		"prefix": ["subscript"],
		"body": [
			"<sub>$1 </sub> $0",
		],
		"description": "Subscript"
	},

	"super": {
		"prefix": ["superscript"],
		"body": [
			"<sup>$1 </sup> $0",
		],
		"description": "Superscript"
	},

		"md.table":{
				"body": [
					"$1| $2"
					"-|-"
					" $3 | $4 "
					"$0"
						],
		}

		"comm.star": {
			"prefix": ["Situation Task Action Response"],
			"body": [
				"Situation: $1"
				"Task: $2"
				"Assessment: $3"
				"Response: $4",
			],
			"description": "Situation Task Action Response"
		},

		"analysis.SWOT": {
			"body": [
				"Strengths: $1"
				"Weaknesses: $2"
				"Opportunites: $3"
				"Threats: $4",
			]
		},

