// Folding
	"fold.selected": {
		"body":
				["<details><summary>$1",
					"  </summary>",
					"  ${TM_SELECTED_TEXT}",
					"  </details>",
					"$0"],
					"description": "Creates a toggle fold on the selection"
				},
	"fold.new": {
		"body":
		["<details><summary>$1",
			"  </summary>",
			"  $2",
			"  </details>",
			"  $0"],
			"description": "Creates a new toggle fold"
		},
	// Markdown text formatting
		"pagebreak": {
			"prefix" : ["Page Break"],
			"body": " <div style="page-break-after: always"></div> ",
			"description": "Creates a Page Break"
		},
		"iframe": {
			"prefix" : ["IFrame"],
			"body": ["<iframe src=$1></iframe>$0"],
			"description": "Inserts an iframe"
		}
		"sub": {
			"prefix": ["subscript"],
			"body": [
				"<sub>$1 </sub> $0",
			],
			"description": "Subscript"
		},
		"super": {
			"prefix": ["superscript"],
			"body": [
				"<sup>$1 </sup> $0",
			],
			"description": "Superscript"
		},
		"md.table":{
			"body": [
				"$1| $2"
				"-|-"
				" $3 | $4 "
				"$0"
			],
		},
		"kbd": {
			"prefix" : ["kbd"],
			"body": ["<kbd>$1</kbd> $0"],
			"description": "type a Keyboard button"
		},
